<?xml version='1.0'?>
<!DOCTYPE fontconfig SYSTEM 'fonts.dtd'>
<fontconfig>
<alias>
    <family>serif</family>
    <prefer><family>Sauce Serif Pro</family></prefer>
  </alias>
  <alias>
    <family>sans-serif</family>
    <prefer><family>Sauce Sans Pro</family></prefer>
  </alias>
  <alias>
    <family>sansserif</family>
    <prefer><family>Sauce Sans Pro</family></prefer>
  </alias>
  <alias>
    <family>sans</family>
    <prefer><family>Sauce Sans Pro</family></prefer>
  </alias>
  <alias>
    <family>monospace</family>
    <prefer><family>SauceCodePro</family></prefer>
  </alias>
</fontconfig>
